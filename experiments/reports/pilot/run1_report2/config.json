{
    "description": "Applying deduplication on default report",
    "emb_model": "thenlper/gte-large",
    "emb_type": "sentence_embedding",
    "emb_strategy": "direct",
    "distance_fn": "cosine",
    "lower": true,
    "remove_punct": true,
    "remove_stopwords": true,
    "lemmatize": true,
    "unique": true,
    "cluster_linkage": "ward",
    "cluster_dist_threshold": 0.5,
    "num_samples": 25,
    "deduplicate": true
}