{
    "description": "Main results grouped by sentience and semantic distance",
    "emb_model": "thenlper/gte-large",
    "emb_type": "sentence_embedding",
    "emb_strategy": "direct",
    "distance_fn": "cosine",
    "lower": true,
    "remove_punct": true,
    "remove_stopwords": true,
    "lemmatize": true,
    "unique": true,
    "cluster_linkage": "ward",
    "cluster_dist_threshold": 0.6,
    "deduplicate": true,
    "filter_by": {
        "sentence_length": [3, 7]
    },
    "group_by": ["sentience", "semantic_distance"],
    "max_sampling": true
}